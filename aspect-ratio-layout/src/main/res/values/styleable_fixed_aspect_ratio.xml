<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="aspect_orientation">
        <enum name="auto" value="0" />
        <enum name="width" value="1" />
        <enum name="height" value="2" />
    </attr>

    <declare-styleable name="FixedAspectRatio">
        <attr name="aspect_width" format="integer" />
        <attr name="aspect_height" format="integer" />
        <attr name="aspect_orientation" />
        <attr name="relative_width" format="float" />
        <attr name="relative_height" format="float" />
    </declare-styleable>
</resources>
